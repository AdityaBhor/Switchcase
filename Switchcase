import java.util.Scanner;
public class SwitchCase {
  
  public static void main(String[]args){
    
    int choice;
    System.out.println("Pick one:1.Pencil\t2.Book\t3.Laptop\t4.Notebook");
    Scanner sc = new Scanner(System.in);
    choice=sc.nextInt();
    switch(choice)
    {
      case 1: System.out.println("I have a Pencil");
            break;
      case 2: System.out.println("I have my Book");
            break;
      case 3: System.out.println("I have my own Laptop");
            break;
      case 4: System.out.println("I have my Notebook with me");
            break;
      case 5: System.out.println("Invaid Choice");   
      
    }
  }
}
 
 
 

